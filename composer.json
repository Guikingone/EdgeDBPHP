{
	"name": "guikingone/edgedb-php",
	"description": "A PHP client for EdgeDB",
	"authors": [
		{
			"name": "Guillaume Loulier",
			"email": "contact@guillaumeloulier.fr",
			"homepage": "https://www.guillaumeloulier.fr/"
		}
	],
	"autoload": {
		"psr-4": {
			"EdgeDB\\": "src/",
			"EdgeDB\\Client\\": "src/Client/",
			"EdgeDB\\Exception\\": "src/Exception/",
			"EdgeDB\\Event\\": "src/Event/",
			"EdgeDB\\Events\\Transaction\\": "src/Events/Transaction/",
			"EdgeDB\\Protocol\\": "src/Protocol/",
			"EdgeDB\\Query\\": "src/Query/",
			"EdgeDB\\Types\\": "src/Types/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\EdgeDB\\": "tests/",
			"Tests\\EdgeDB\\Event\\": "tests/Event/",
			"Tests\\EdgeDB\\Events\\Query\\": "tests/Events/Query/",
			"Tests\\EdgeDB\\Events\\Transaction\\": "tests/Events/Transaction/"
		}
	},
	"require": {
		"php": ">=7.4",
		"ext-json": "*",
		"symfony/options-resolver": "^5.2",
		"psr/event-dispatcher": "^1.0",
		"psr/http-client": "^1.0",
		"psr/http-message": "^1.0",
		"psr/http-factory": "^1.0",
		"php-http/discovery": "^1.13",
		"psr/log": "^1.1",
		"webonyx/graphql-php": "^14.5"
	},
	"require-dev": {
		"composer/package-versions-deprecated": "^1.11",
		"friendsofphp/php-cs-fixer": "^2.17",
		"infection/infection": "^0.21",
		"nikic/php-parser": "^4.10.3",
		"phpstan/phpstan": "^0.12",
		"phpstan/phpstan-phpunit": "^0.12.17",
		"phpstan/phpstan-strict-rules": "^0.12.9",
		"phpunit/phpunit": "^9.5",
		"psr/cache": "^1.0",
		"rector/rector": "^0.9.31",
		"symfony/http-client": "^5.2",
		"guzzlehttp/guzzle": "^7.2",
		"nyholm/psr7": "^1.3",
		"php-http/httplug": "^2.2",
		"vimeo/psalm": "^4.6"
	}
}
