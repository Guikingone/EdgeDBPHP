{
	"name": "guikingone/edgedb-php",
	"description": "A PHP client for EdgeDB",
	"authors": [
		{
			"name": "Guillaume Loulier",
			"email": "contact@guillaumeloulier.fr",
			"homepage": "https://www.guillaumeloulier.fr/"
		}
	],
	"autoload": {
		"psr-4": {
			"EdgeDB\\": "src/",
			"EdgeDB\\Exception\\": "src/Exception/",
			"EdgeDB\\Events\\": "src/Events/",
			"EdgeDB\\Protocol\\": "src/Protocol/",
			"EdgeDB\\Query\\": "src/Query/",
			"EdgeDB\\Types\\": "src/Types/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\EdgeDB\\": "tests/",
			"Tests\\EdgeDB\\Events\\": "tests/Events/"
		}
	},
	"require": {
		"php": ">=7.4",
		"ext-json": "*",
		"ext-pgsql": "*",
		"symfony/options-resolver": "^5.2",
		"psr/http-client": "^1.0",
		"psr/http-message": "^1.0",
		"psr/event-dispatcher": "^1.0",
		"psr/log": "^1.1",
		"psr/http-factory": "^1.0",
		"php-http/discovery": "^1.13",
		"webonyx/graphql-php": "^14.5"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^2.17",
		"infection/infection": "^0.15.3",
		"nikic/php-parser": "4.10.2",
		"phpunit/phpunit": "^8.5",
		"phpstan/phpstan": "^0.12.62",
		"phpstan/phpstan-phpunit": "^0.12.17",
		"rector/rector": "^0.8.56",
		"symfony/http-client": "^5.2",
		"guzzlehttp/guzzle": "^7.2",
		"nyholm/psr7": "^1.3",
		"php-http/httplug": "^2.2",
		"vimeo/psalm": "^4.4"
	}
}
